{
  "name": "fyn",
  "version": "0.2.5",
  "description": "A fast node package manager for better productivity and efficiency",
  "preferGlobal": true,
  "main": "./bin/fyn.js",
  "scripts": {
    "test": "clap -n -x create-tgz electrode/test",
    "coverage": "clap -n -x create-tgz check",
    "ci": "clap -n -x bundle coverage",
    "prepublishOnly": "echo Do not run npm publish directly.  Run clap release. && exit 1",
    "create-tgz": "node test/fixtures/mock-npm/create-tgz"
  },
  "bin": {
    "fyn": "./bin/fyn.js"
  },
  "files": [
    "bin",
    "dist",
    "node-gyp"
  ],
  "keywords": [
    "node",
    "flat",
    "node_modules",
    "module",
    "modules",
    "fast",
    "package",
    "manager",
    "ehanced",
    "local",
    "better",
    "improve",
    "npm",
    "link",
    "development",
    "workflow",
    "producitivy",
    "efficient",
    "disk",
    "space",
    "usage"
  ],
  "author": "Joel Chen <joel123@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/fyn.git"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "chalk": "^2.3.0",
    "es6-promisify": "./es6-promisify",
    "flat-module": "^1.0.0",
    "ignore-loader": "^0.1.2",
    "ini": "^1.3.5",
    "item-queue": "^1.0.3",
    "lockfile": "^1.0.4",
    "lodash": "^4.17.10",
    "mississippi": "^3.0.0",
    "mkdirp": "^0.5.1",
    "nix-clap": "^1.3.4",
    "node-gyp": "3.8.0",
    "npm-lifecycle": "^2.1.0",
    "npm-packlist": "^1.1.11",
    "opfs": "^1.1.0",
    "optional-require": "^1.0.0",
    "pacote": "^9.1.0",
    "pkg-preper": "^0.1.0",
    "rimraf": "^2.6.2",
    "semver": "^5.3.0",
    "tar": "^4.0.0",
    "visual-exec": "^0.1.0",
    "visual-logger": "^0.1.5",
    "xenv-config": "^1.3.0",
    "xsh": "^0.4.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "babel-loader": "^8.0.4",
    "@babel/preset-env": "^7.2.0",
    "electrode-archetype-njs-module-dev": "^3.0.0",
    "electrode-server": "^1.7.0",
    "mock-require": "^2.0.2",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "xstdout": "^0.1.1"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "coverage",
      "*clap.js",
      "gulpfile.js",
      "dist",
      "test",
      "xout",
      "tmp",
      "flat-module.js",
      "webpack.config.js",
      "bin",
      "dist",
      "stubs",
      "*/pkg-bin-linker-win32.js",
      "es6-promisify"
    ]
  },
  "engines": {
    "node": ">=6"
  },
  "opfs": {
    "mkdirp": true,
    "rimraf": true,
    "bluebird": true
  }
}
